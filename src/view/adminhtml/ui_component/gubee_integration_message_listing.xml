<?xml version="1.0" ?>
<listing xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <argument name="data" xsi:type="array">
        <item name="js_config" xsi:type="array">
            <item name="provider" xsi:type="string">gubee_integration_message_listing.gubee_integration_queue_message_listing_data_source</item>
        </item>
    </argument>
    <settings>
        <spinner>gubee_integration_message_columns</spinner>
        <deps>
            <dep>gubee_integration_message_listing.gubee_integration_queue_message_listing_data_source</dep>
        </deps>
    </settings>
    <dataSource name="gubee_integration_queue_message_listing_data_source" component="Magento_Ui/js/grid/provider">
        <settings>
            <storageConfig>
                <param name="indexField" xsi:type="string">message_id</param>
            </storageConfig>
            <updateUrl path="mui/index/render"/>
        </settings>
        <aclResource>Gubee_Integration::Message</aclResource>
        <dataProvider name="gubee_integration_queue_message_listing_data_source" class="Magento\Framework\View\Element\UiComponent\DataProvider\DataProvider">
            <settings>
                <requestFieldName>id</requestFieldName>
                <primaryFieldName>message_id</primaryFieldName>
            </settings>
        </dataProvider>
    </dataSource>
    <listingToolbar name="listing_top">
        <settings>
            <sticky>true</sticky>
        </settings>
        <bookmark name="bookmarks"/>
        <columnsControls name="columns_controls"/>
        <filters name="listing_filters"/>
        <paging name="listing_paging"/>
    </listingToolbar>
    <columns name="gubee_integration_message_columns">
        <selectionsColumn name="ids">
            <settings>
                <indexField>message_id</indexField>
            </settings>
        </selectionsColumn>
        <column name="message_id">
            <settings>
                <filter>text</filter>
                <sorting>asc</sorting>
                <label translate="true">ID</label>
            </settings>
        </column>
        <column name="command">
            <settings>
                <filter>text</filter>
                <label translate="true">command</label>
            </settings>
        </column>
        <column name="priority">
            <settings>
                <filter>text</filter>
                <label translate="true">priority</label>
            </settings>
        </column>
        <column name="status" class="Gubee\Integration\Ui\Component\Message\Listing\Column\Status">
            <settings>
                <label translate="true">Status</label>
                <bodyTmpl>ui/grid/cells/html</bodyTmpl>
            </settings>
        </column>
        <column name="payload">
            <settings>
                <label translate="true">Payload</label>
            </settings>
        </column>
        <column name="attempts">
            <settings>
                <label translate="true">Attempts</label>
            </settings>
        </column>
        <column name="message">
            <settings>
                <label translate="true">Message</label>
            </settings>
        </column>
        <column name="created_at">
            <settings>
                <label translate="true">Created At</label>
            </settings>
        </column>
        <column name="updated_at">
            <settings>
                <label translate="true">Updated At</label>
            </settings>
        </column>
        <column name="details" class="Gubee\Integration\Ui\Component\Message\Listing\Column\Details">
            <settings>
                <label translate="true">Details</label>
                <bodyTmpl>ui/grid/cells/html</bodyTmpl>

            </settings>
        </column>
        <actionsColumn name="actions" class="Gubee\Integration\Ui\Component\Message\Listing\Column\Action">
            <settings>
                <indexField>message_id</indexField>
            </settings>
        </actionsColumn>

    </columns>
</listing>