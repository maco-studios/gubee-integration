<?xml version="1.0" encoding="UTF-8"?>
<listing xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">

    <listingToolbar name="listing_top">
        <massaction name="listing_massaction">
            <action name="gubee_send">
                <argument name="data" xsi:type="array">
                    <item name="config" xsi:type="array">
                        <item name="type" xsi:type="string">gubee_send</item>
                        <item name="label" xsi:type="string" translate="true">Send to Gubee</item>
                        <item name="url" xsi:type="url" path="gubee/catalog_product/send"/>
                        <item name="confirm" xsi:type="array">
                            <item name="title" xsi:type="string" translate="true">Are you sure?</item>
                            <item name="message" xsi:type="string" translate="true">Are you sure you want to do this action?</item>
                        </item>
                    </item>
                </argument>
            </action>
        </massaction>
    </listingToolbar>
</listing>
